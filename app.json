{
  "expo": {
    "name": "CarApp",
    "slug": "car-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carapp.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.carapp.app"
    },
    "web": {
      "favicon": "./favicon.ico"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
